<% if user_signed_in? %>
  <%= form_with url: search_path, method: :get, local: true do |f| %>
    <%= f.text_field :content %>
    <%= f.select :model, options_for_select({ "ユーザー" => "user", "ウォント" => "want_clothes", "リコメンド" => "recommend" }) %>
    <%= f.submit 'seach', class: "btn btn-default" %>
  <% end %>
<% end %>